{
  "title": "Heartland virus L segment phylogenetic analysis",
  "maintainers": [
    {"name": "ZP", "url": "none"}
  ],
  "build_url": "https://github.com/ZP/heartland-nextstrain",
  "colorings": [
    {
      "key": "gt",
      "title": "Genotype",
      "type": "categorical"
    },
    {
      "key": "num_date",
      "title": "Date",
      "type": "continuous"
    },
    {
      "key": "country",
      "title": "Country",
      "type": "categorical"
    },
    {
      "key": "state",
      "title": "State",
